{
  "name": "wgx954418992/php-baidubce",
  "description": "BCE SDK for PHP",
  "keywords": [
    "百度云sdk",
    "baidu",
    "baidu-bce",
    "bce",
    "baidu",
    "sdk"
  ],
  "type": "library",
  "homepage": "https://cloud.baidu.com/doc/Developer/index.html?sdk=php",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Renew",
      "email": "wgx954418992@qq.com"
    },
    {
      "name": "Baidu BCE",
      "homepage": "https://bce.baidu.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "ext-json": "*",
    "guzzle/guzzle": "^3.9"
  },
  "autoload": {
    "files": [],
    "psr-4": {
      "BaiduBce\\": "src/BaiduBce/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  },
  "support": {
    "issues": "https://github.com/baidubce/bce-sdk-php/issues"
  },
  "suggest": {
    "monolog/monolog": "Adds support for logging HTTP requests and responses"
  },
  "require-dev": {
    "monolog/monolog": "^1.26",
    "phpunit/phpunit": "^7.0"
  },
  "scripts": {
    "extract": [
      "php -r '$phar = new Phar(\"./BaiduBce.phar\");$result = $phar->extractTo(\"./\",\"src/\",true); echo $result ? \"success\":\"fail\";'"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.18-dev"
    }
  }
}
